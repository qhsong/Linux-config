!颜色设置
URxvt.depth:32
URxvt.colorBD:white
URxvt.colorUL:Green
URxvt.background: black
URxvt.foreground: white
!!!调整透明度
URxvt.inheritPixmap: True
URxvt.tintColor: grey
URxvt.shading: 60
!!URL操作
URxvt.urlLauncher:firefox
URxvt.matcher.button:1
!!滚动条设置
URxvt.scrollBar:False
URxvt.scrollBar_right:True
URxvt.scrollBar_floating:False
URxvt.scrollstyle:plain
URxvt.saveLines:5000
!!滚屏设置
URxvt.mouseWheelScrollPage:True
URxvt.scrollTtyOutput:False
URxvt.scrollWithBuffer:True
URxvt.scrollTtyKeypress:True
!!光标闪烁
URxvt.cursorBlink:True

Xft.dpi:96
URxvt.font:xft:文泉驿等宽微米黑:size=11:style=Regular:antialias=true,xft:文泉驿等宽微米黑:size=11:style=Regular:antialias=true
URxvt.boldfont:xft:文泉驿等宽微米黑:size=11:style=Bold:antialias=true,xft:文泉驿等宽微米黑:size=11:style=Bold:antialias=true

URxvt.perl-lib:/home/qhsong/.urxvt/ext
URxvt.perl-ext-common: default,clipboard
URxvt.keysym.Shift-Control-C: perl:clipboard:copy
URxvt.keysym.Shift-Control-V: perl:clipboard:paste

